<!DOCTYPE html/><html><head>    <title><%= title %></title>    <% styles.forEach(function (style) { %>    <link rel="stylesheet" href="<%= style %>" />    <% }) %>    <script type="text/javascript">        var themeColour = 'white';    </script>    <% scripts.forEach(function (script) { %>        <script type="text/javascript" src="<%= script %>"></script>    <% }) %></head><body>    <div class="stat">        <ul class="nav nav-tabs stat-nav" role="tablist">            <li class="dropdown">                <a class="dropdown-toggle btn-primary" data-toggle="dropdown" href="#">  select datasource  <span class="caret"></span></a>                <ul class="dropdown-menu" role="menu">                    <% dataSources.forEach(function (dataSource) { %>                    <li><a href="#"><%= dataSource.name %></a></li>                    <% }) %>                </ul>            </li>            <li class="dropdown">                <a class="dropdown-toggle" data-toggle="dropdown" href="#">                selet period<span class="caret"></span>                </a>                <ul class="dropdown-menu datasource-menu" role="menu">                    <li><a href="#">今日</a></li>                    <li><a href="#">昨日</a></li>                    <li><a href="#">7天</a></li>                    <li><a href="#">30天</a></li>                    <li><a href="#">90天</a></li>                    <li><a href="#">180天</a></li>                    <li><a href="#">360天</a></li>                </ul>            </li>        </ul>        <div class="stat-chart" data-config='<%= JSON.stringify(widget.config) %>'>            <div class="content">            </div>        </div>        <div class="stat-data">            <% multiRecords.forEach(function( obj ) {%>            <div class="panel panel-info">                <div class="panel-heading">                    <h3 class="panel-title"><%= obj.name %></h3>                </div>                <div class="panel-body">                    <table class="table table-hover">                        <thead>                        <tr>                            <td class="time">Year</td>                            <td class="time">Month</td>                            <td class="time">Day</td>                            <td class="time">Time</td>                            <td>Value</td>                        </tr>                        </thead>                        <tbody>                        <% obj.data.forEach(function(record) { %>                        <tr>                            <td class="time"><%= record.year %></td>                            <td class="time"><%= record.month %></td>                            <td class="time"><%= record.day %></td>                            <td class="time"><%= record.hour || '00' %> : <%= record.minute || '00' %> : <%= record.second || '00' %></td>                            <td><%= record.value %></td>                        </tr>                        <% }) %>                        </tbody>                    </table>                </div>              </div>            <% }) %>        </div>    </div></body></html>