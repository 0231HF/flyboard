<div class="widget" data-config='<%= JSON.stringify(widget.config) %>' data-type="3">    <header>        <p><span></span><a href="<%= widgetUrl(widget) %>"><%= widget.config.name %></a></p>    </header>    <div class="content cf-svp clearfix" id="svp-<%= widget.id %>">        <div class="chart" data-percent="0">            <div class="metrics">                <span class="metric"></span>                <span class="metric-small">%</span>            </div>        </div>    </div></div>