{
  "name": "flyboard",
  "version": "0.0.1",
  "private": true,
  "author": "perfectworks <perfectworks@gmail.com",
  "contributors": [
    "slycloud <sky_zuoyou@sina.com>"
  ],
  "scripts": {
    "start": "node ./bin/www",
    "test-jshint": "./node_modules/.bin/jshint src views bin configs",
    "test": "./node_modules/.bin/mocha --recursive",
    "test-coverage": "./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "migrate:latest": "./node_modules/.bin/knex --knexfile ./configs/database.js --cwd . migrate:latest",
    "migrate:make": "./node_modules/.bin/knex --knexfile ./configs/database.js --cwd . migrate:make flyboard",
    "migrate:rollback": "./node_modules/.bin/knex --knexfile ./configs/database.js --cwd . migrate:rollback",
    "migrate:currentVersion": "./node_modules/.bin/knex --knexfile ./configs/database.js --cwd . migrate:currentVersion"
  },
  "main": "app.js",
  "dependencies": {
    "bluebird": "^2.1.3",
    "body-parser": "~1.0.0",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "express": "~4.2.0",
    "jade": "~1.3.0",
    "jshint": "^2.5.1",
    "knex": "^0.6.17",
    "morgan": "~1.0.0",
    "node-uuid": "^1.4.1",
    "static-favicon": "~1.0.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.1",
    "coveralls": "^2.10.1",
    "mariasql": "^0.1.21",
    "mocha": "^1.20.1",
    "mocha-lcov-reporter": "0.0.1",
    "mysql": "^2.3.2",
    "pq": "0.0.3",
    "sqlite3": "^2.2.3",
    "supertest": "^0.13.0"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
