    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-mysql-session": "^0.4.1",
    "express-session": "^1.11.3",
    "iconv-lite": "^0.4.13",
    "jade": "^1.11.0",
    "jshint": "^2.8.0",
    "jwt-simple": "^0.3.1",
    "knex": "^0.8.6",
    "ldapjs": "^0.7.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "node-uuid": "^1.4.3",
    "passport": "^0.3.0",
    "passport-github": "^1.0.0",
    "passport-google": "^0.3.0",
    "passport-ldapauth": "^0.3.1",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.0",
    "serve-favicon": "^2.3.0",
