    "mocha-cov-reporter": "^0.2.3",
    "mocha-lcov-reporter": "0.0.1",
    "mysql": "^2.3.2",
    "pq": "0.0.3",
    "supertest": "^0.13.0",
    "supertest-as-promised": "^1.0.0"
